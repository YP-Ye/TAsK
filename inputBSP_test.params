<PARSER>: {DEFAULT} // possible values: DEFAULT - for usual parsing
 			//            ART2 - for ART2
<NETWORK>: {../Data/InstancesWithTolls/Random/Anaheim_tolls.txt}

<OD_MATRIX>: {../Data/Instances/Anaheim_trips.txt} //path to file with O-D matrix

<WRITE_RND_TOLLS_TO_FILE>: {} // if specified instead of tolls from file, random ones will be used

<INDIFF_CURVE_TYPE>: {PiecewiseLinear} //PiecewiseLinear
<INDIFF_CURVE>: {Curves/Anaheim_tolls.curves} // path to file with indifference curves, if not specified the problem is considered to be additive NOT IMPLEMENTED
// Curves/SiouxFalls_tolls.curves
			// use RND_GEN - to generate curves randomly
// If random curves are used these parameters must be specified
<MAX_NB_POINTS_PER_CURVE>: {3}
<MAX_TOLL> : {20}
<MAX_TIME> : {10}

/*************************************************************************************************************
 ** Algorithms and Parameters 
 *************************************************************************************************************/
<TIME_LIMIT> : {500000} // in seconds
<ShPathAlgo> : {NonAdd} // LC, Astar, NonAdd, LazyNonAdd, NonAddWithAstar
<UseP2PShPathWithRandomReturn>: {} // if empty, not considered.
<FIXED_PROBABILITY>: {}
<CONV_MEASURE>: {MAX_DIFF} // convergence measure; possible values: RGAP, MAX_DIFF
<PRECISION>: {1e-5} // algorithm precision
<ZERO_FLOW>: {1e-10} // zero flow tolerance
<DIR_TOLERANCE>: {1e-10} // is compulsory for any path- and origin-based algo, tolerance of descent direction
<ALGORITHM>: {PE} // algorithm and coressponding parameters must be spisified. Possible values:
			<EQUILIBRATION>: {EQI} // type of equilibration, possible values: EQI, EQII
			<APPROACH>: {APP3} // approach, possible values: APP1, APP3
			<ALPHA>: {0.25}
			<LINE_SEARCH>: {QUAD_APP} // line search, possible values: BISEC, ARMIJO, QUAD_APP
				<LS_PRECISION>: {1e-15} // line serch precision
				<ARMIJO_DEC>: {2} 
			<ISP_SCALE>: {0.15} 
			<SLOPE>: {1e-15}
		
<BIOBJ_SHPATH_ONE_SOURCE>: {BiLabelSetting} // possible values: Andrea_BiLabelCorrecting, 
												// BiLabelSetting, BiLabelSetting_p2pbased
<BIOBJ_SHPATH_P2P>: {BiLabelSetting} // possible values: Andrea_BiLabelCorrecting, BiLabelSetting, 
												// BiLabelSetting_bidirectional
<BLS_BOUNDS> : {currentFlow} // Possible values: zeroFlow, currentFlow
<USE_EXISTING_PATHS> : {} // If empty no paths are added, otherwise: onePath - only shortest path is added
									// currentPaths - all existing paths are added
<USE_PATH_COST_DOMINATION> : {} // If empty ususal domination rule is used, otherwise domination
							// by path cost is used

/*************************************************************************************************************		
 ** Screen Output 
 *************************************************************************************************************/
<SHOW_CONVERGENCE>: {yes} // prints convergence on the screen, possible values: any thing that is not emty string

/*************************************************************************************************************
 ** Output Files 
 *************************************************************************************************************/
 //art2rndTolls.flows
<LINK_FLOWS>: {} // path to file where link flows must be written. If set AUTO, automatically generated name will be used
<CONVERGENCE>: {} // path to file where convergence must be written. If set AUTO, automatically generated name will be used
<COUNTERS>: {} // path to file where all counters must be writtem. If set AUTO or nothing, automatically generated name will be used
<PATH_SET>: {}
<NODES>: {}
